    "contracts/TwoCrazySnake/RandomNumberGenerator.sol": {
      "content": "// SPDX-License-Identifier: MIT\npragma solidity ^0.8.4;\n\nimport \"@openzeppelin/contracts-upgradeable/proxy/utils/Initializable.sol\";\nimport \"@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol\";\nimport \"@openzeppelin/contracts-upgradeable/token/ERC20/utils/SafeERC20Upgradeable.sol\";\nimport \"@chainlink/contracts/src/v0.8/interfaces/LinkTokenInterface.sol\";\nimport \"@chainlink/contracts/src/v0.8/interfaces/VRFCoordinatorV2Interface.sol\";\nimport \"./IRandomNumberGenerator.sol\";\nimport \"./ITwoCrazySnake.sol\";\n\ncontract RandomNumberGenerator is IRandomNumberGenerator, OwnableUpgradeable {\n\n    address public twoCrazySnake;\n    bytes32 public keyHash;\n    uint256 public latestRequestId;\n    uint32 public redScore;\n    uint32 public blueScore;\n\n    uint256 public currentRoundId;\n    uint256 public latestRoundId;\n\n    // A reasonable default is 100000, but this value could be different\n    // on other networks.\n    uint32 public callbackGasLimit;\n\n    // The default is 3, but you can set this higher.\n    uint16 private requestConfirmations;\n\n    // For this example, retrieve 2 random values in one request.\n    // Cannot exceed VRFCoordinatorV2.MAX_NUM_WORDS.\n    uint32 private numWords;\n\n    uint64 private subscriptionId;\n\n    error OnlyCoordinatorCanFulfill(address have, address want);\n\n    // address of VRFCoordinator contract\n    address public vrfCoordinator;\n\n    function initialize(address _vrfCoordinator) public initializer {\n        __Ownable_init_unchained();\n\n        vrfCoordinator = _vrfCoordinator;\n        callbackGasLimit = 100000;\n        //2022.10.28\n        requestConfirmations = 3;\n        numWords = 2;\n    }\n\n    // rawFulfillRandomness is called by VRFCoordinator when it receives a valid VRF\n    // proof. rawFulfillRandomness then calls fulfillRandomness, after validating\n    // the origin of the call\n    function rawFulfillRandomWords(\n        uint256 requestId,\n        uint256[] memory randomWords\n    ) external {\n        if (msg.sender != vrfCoordinator) {\n            revert OnlyCoordinatorCanFulfill(msg.sender, vrfCoordinator);\n        }\n        fulfillRandomWords(requestId, randomWords);\n    }\n\n    /**\n     * @notice Request randomness\n     */\n    function getRandomNumber(uint256 _currentRoundId) external override {\n        require(msg.sender == twoCrazySnake, \"Only twoCrazySnake\");\n        require(keyHash != bytes32(0), \"Must have valid key hash\");\n\n        currentRoundId = _currentRoundId;\n\n        latestRequestId = VRFCoordinatorV2Interface(vrfCoordinator)\n            .requestRandomWords(\n                keyHash,\n                subscriptionId,\n                requestConfirmations,\n                callbackGasLimit,\n                numWords\n            );\n    }\n\n    /**\n     * @notice Change the subscription id\n     * @param _subscriptionId: subscription id\n     */\n    function setSubscriptionId(uint64 _subscriptionId) external onlyOwner {\n        subscriptionId = _subscriptionId;\n    }\n\n    /**\n     * @notice Change the callback gas Limit\n     * @param gasLimit: callback gas Limit\n     */\n    function setCallbackGasLimit(uint32 gasLimit) external onlyOwner {\n        callbackGasLimit = gasLimit;\n    }\n\n    /**\n     * @notice Change the keyHash\n     * @param _keyHash: new keyHash\n     */\n    function setKeyHash(bytes32 _keyHash) external onlyOwner {\n        keyHash = _keyHash;\n    }\n\n    /**\n     * @notice Set the address for the twoCrazySnake\n     * @param _twoCrazySnake: address of the twoCrazySnake contract\n     */\n    function setCrazySnakeAddress(address _twoCrazySnake) external onlyOwner {\n        twoCrazySnake = _twoCrazySnake;\n    }\n\n    /**\n     * @notice View latestRoundId\n     */\n    function viewLatestRoundId() external view override returns (uint256) {\n        return latestRoundId;\n    }\n\n    /**\n     * View latest request id\n     */\n    function viewLatestRequestId() external view override returns (uint256) {\n        return latestRequestId;\n    }\n\n    /**\n     * @notice View random result\n     */\n    function viewRandomResult()\n        external\n        view\n        override\n        returns (uint32 _redScore, uint32 _blueScore)\n    {\n        _redScore = redScore;\n        _blueScore = blueScore;\n    }\n\n    /**\n     * @notice Callback function used by ChainLink's VRF Coordinator\n     */\n    function fulfillRandomWords(uint256 requestId, uint256[] memory randomWords)\n        internal\n    {\n        require(latestRequestId == requestId, \"Wrong requestId\");\n        redScore = uint32((randomWords[0] + block.timestamp) % 1000000);\n        blueScore = uint32((randomWords[1] + block.timestamp) % 1000000);\n        latestRoundId = ITwoCrazySnake(twoCrazySnake).viewCurrentRoundId();\n        require(latestRoundId == currentRoundId, \"Wrong roundId\");\n        ITwoCrazySnake(twoCrazySnake).setRoundOracleCalled(latestRoundId);\n    }\n}\n"
    },
    "contracts/TwoCrazySnake/IRandomNumberGenerator.sol": {
      "content": "// SPDX-License-Identifier: MIT\npragma solidity ^0.8.4;\n\ninterface IRandomNumberGenerator {\n    /**\n     * Requests randomness\n     */\n    function getRandomNumber(uint256 _currentRoundId) external;\n\n    /**\n     * View latest lotteryId numbers\n     */\n    function viewLatestRoundId() external view returns (uint256);\n\n    /**\n     * View latest request id\n     */\n    function viewLatestRequestId() external view returns (uint256);\n\n    /**\n     * Views random result\n     */\n    function viewRandomResult()\n        external\n        view\n        returns (uint32 _redSankeScore, uint32 _blueSankeScore);\n}\n"
    },
    "contracts/TwoCrazySnake/ITwoCrazySnake.sol": {
      "content": "// SPDX-License-Identifier: MIT\npragma solidity >=0.8.4;\n\ninterface ITwoCrazySnake {\n    enum Position {\n        Null,\n        Red,\n        Blue\n    }\n    enum Status {\n        Pending,\n        Open,\n        Close,\n        Claimable\n    }\n\n    struct Round {\n        uint256 roundId;\n        uint256 startTimestamp;\n        uint256 closeTimestamp;\n        uint256 totalAmount;\n        uint256 redAmount;\n        uint256 blueAmount;\n        bool oracleCalled;\n        Status status;\n        uint32 redScore;\n        uint32 blueScore;\n        uint32 redUsers;\n        uint32 blueUsers;\n        uint256 rewardBaseCalAmount;\n        uint256 rewardAmount;\n    }\n\n    struct BetInfo {\n        Position position;\n        bool claimed; // default false\n        uint256 amount;\n    }\n\n    //-----------------------------------------------------------------------------\n    // events\n    //-----------------------------------------------------------------------------\n    event Bet(\n        address indexed user,\n        uint256 indexed roundId,\n        uint256 pos,\n        uint256 amount\n    );\n    event Claim(address indexed user, uint256 indexed roundId, uint256 amount);\n    event StartRound(\n        uint256 indexed roundId,\n        uint256 startTimestamp,\n        uint256 closeTimestamp\n    );\n    event CloseRound(\n        uint256 indexed roundId\n    );\n    event FillFinalScore(\n        uint256 indexed roundId,\n        uint32 redScore,\n        uint32 blueScore\n    );\n    event RewardsCalculated(\n        uint256 indexed roundId,\n        uint256 rewardBaseCalAmount,\n        uint256 rewardAmount,\n        uint256 treasuryAmount\n    );\n    event RoundOracleCalled(uint256 indexed roundId);\n    event TreasuryClaim(uint256 amount);\n    event NewMinBetAmount(uint256 indexed roundId, uint256 minBetAmount);\n    event NewOracle(address indexed oracle);\n    event NewTreasuryFee(uint256 indexed roundId, uint256 treasuryFee);\n    event NewOperator(address indexed operator);\n\n    //-----------------------------------------------------------------------------\n    // write interface.\n    //-----------------------------------------------------------------------------\n    function bet(\n        uint256 _roundId,\n        Position _pos,\n        uint256 _amount\n    ) external;\n\n    function claim(uint256[] calldata _roundIds) external;\n\n    function startRound() external;\n\n    function closeRound() external;\n\n    function executeRound(bool bAutoNext) external;\n\n    //-----------------------------------------------------------------------------\n    // admin set interface: onlyRole(CONFIG_ADMIN_ROLE)\n    //-----------------------------------------------------------------------------\n    function setConfig(\n        address _token,\n        address _oracleAddress,\n        address _treasuryAddress,\n        uint256 _treasuryFee,\n        uint256 _minBetAmount,\n        uint256 _intervalSeconds\n    ) external;\n\n    function getConfig()\n        external\n        view\n        returns (\n            address,\n            address,\n            address,\n            uint256,\n            uint256,\n            uint256\n        );\n\n    function setRoundOracleCalled(uint256 _roundId) external;\n\n    function claimTreasury() external;\n\n    function setMinBetAmount(uint256 _minBetAmount) external;\n\n    function setOracle(address _oracle) external;\n\n    function setTreasuryFee(uint256 _treasuryFee) external;\n\n    function setOperator(address _operator) external;\n\n    //-----------------------------------------------------------------------------\n    // view interface.\n    //-----------------------------------------------------------------------------\n    function viewCurrentRoundId() external view returns (uint256);\n\n    function isOracleCalled(uint256 _roundId) external view returns (bool);\n\n    function getUserRounds(\n        address user,\n        uint256 cursor,\n        uint256 size\n    )\n        external\n        view\n        returns (\n            uint256[] memory,\n            BetInfo[] memory,\n            uint256\n        );\n\n    function getUserRoundsLength(address user) external view returns (uint256);\n\n    function getRoundBetInfo(uint256 _round)\n        external\n        view\n        returns (BetInfo[] memory);\n\n    function getRoundUsers(uint256 _round)\n        external\n        view\n        returns (address[] memory);\n\n    function getRoundUsersLength(uint256 _round)\n        external\n        view\n        returns (uint256);\n\n    function getRound(uint256 roundId) external view returns (Round memory);\n}\n"
    },
