        uint j = 0;

        uint[] memory tokensId = new uint[](ids.length);

        while (i < ids.length) 
        { 
            uint itemId = ids[i];
            /**
            * @dev check the number has already been purchased
            */
            if(indexTokens[itemId] == 0)
