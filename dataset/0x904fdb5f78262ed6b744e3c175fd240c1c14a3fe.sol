       /**
       * @dev transfers the fee amount to the office contract
       */
       IERC20(token).transferFrom(msg.sender, _office, fee);

       /**
       * @dev transfers the fee amount to the office contract
       */
       IERC20(token).transferFrom(msg.sender, address(this), total.sub(fee));
      
    }
    /// @notice mint the nft of the purchased number
