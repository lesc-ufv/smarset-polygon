    "contracts/Publication.sol": {
      "content": "// SPDX-License-Identifier: MIT\n\npragma solidity 0.8.7;\n\nimport \"@openzeppelin/contracts/token/ERC721/extensions/ERC721URIStorage.sol\";\nimport \"@openzeppelin/contracts/access/AccessControl.sol\";\n\nimport {ERC721Enumerable} from \"./base/ERC721Enumerable.sol\";\nimport {Errors} from \"./libraries/Errors.sol\";\nimport \"./IPublication.sol\";\n\n/**\n Writers create Publication NFT-s. These are owned by writers\n Publications to store publication metadata.\n */\ncontract Publication is ERC721Enumerable, AccessControl, IPublication {\n    event PublicationAdded(uint256 tokenId);\n\n    bytes32 public constant WRITER_ROLE = keccak256(\"WRITER_ROLE\");\n\n    uint256 s_tokenCounter;\n\n    mapping(uint256 => PublicationInfo) s_publications;\n\n    bool private _initialized;\n\n    function initialize(string calldata name, string calldata symbol) external {\n        if (_initialized) revert Errors.Initialized();\n        _setupRole(DEFAULT_ADMIN_ROLE, msg.sender);\n        _setupRole(WRITER_ROLE, msg.sender);\n\n        s_tokenCounter = 0;\n        _initialized = true;\n        super.__ERC721_Init(name, symbol);\n    }\n\n    /**\n     * ADMIN\n     */\n\n    /**\n     * Add a new writer that can add new publications.\n     */\n    function addWriter(address writer) external onlyRole(DEFAULT_ADMIN_ROLE) {\n        _grantRole(WRITER_ROLE, writer);\n    }\n\n    /**\n     * Remove a writer\n     */\n    function removeWriter(address writer) external onlyRole(DEFAULT_ADMIN_ROLE) {\n        _revokeRole(WRITER_ROLE, writer);\n    }\n\n    function withdraw() external onlyRole(DEFAULT_ADMIN_ROLE) {\n        uint256 amount = address(this).balance;\n        (bool success, ) = payable(msg.sender).call{value: amount}(\"\");\n        require(success, \"Transfer failed\");\n    }\n\n    function setMintFee(uint256 publicationId, uint256 mintFee) external {\n        // check if sender is owner of token\n        require(ownerOf(publicationId) == msg.sender, \"Only owner can set mint fee\");\n        s_publications[publicationId].mintFee = mintFee;\n    }\n\n    /**\n     * WRITER\n     */\n\n    /**\n     * Add a publication.\n     * @param publication IPFS uri with metadata and image url.\n     */\n    function addPublication(PublicationInfo memory publication) public onlyRole(WRITER_ROLE) {\n        uint256 tokenId = ++s_tokenCounter;\n        _mint(msg.sender, s_tokenCounter);\n        s_publications[tokenId] = publication;\n        emit PublicationAdded(s_tokenCounter);\n    }\n\n    /**\n     * IPublication\n     */\n    function getPublicationInfo(uint256 tokenId)\n        public\n        view\n        override\n        returns (PublicationInfo memory)\n    {\n        if (!_exists(tokenId)) revert Errors.TokenDoesNotExist();\n        return s_publications[tokenId];\n    }\n\n    /**\n     * IERC721\n     */\n    function tokenURI(uint256 tokenId) public view override returns (string memory) {\n        IPublication.PublicationInfo memory publication = getPublicationInfo(tokenId);\n        return publication.uri;\n    }\n\n    function _beforeTokenTransfer(\n        address from,\n        address to,\n        uint256 tokenId\n    ) internal virtual override {\n        // allow mint and burn only\n        if (from != address(0x0) && to != address(0x0)) revert Errors.NonTransferrable();\n        super._beforeTokenTransfer(from, to, tokenId);\n    }\n\n    function supportsInterface(bytes4 interfaceId)\n        public\n        view\n        virtual\n        override(ERC721Enumerable, AccessControl, IERC165)\n        returns (bool)\n    {\n        return super.supportsInterface(interfaceId);\n    }\n}\n"
    },
