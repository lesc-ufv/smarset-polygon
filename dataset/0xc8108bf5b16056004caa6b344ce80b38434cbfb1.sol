    "contracts/skyverse-marketplace.sol": {
      "content": "//SPDX-License-Identifier: UNLICENSED\npragma solidity ^0.8.7;\n\nimport \"@openzeppelin/contracts-upgradeable/token/ERC1155/ERC1155Upgradeable.sol\";\nimport \"@openzeppelin/contracts-upgradeable/access/OwnableUpgradeable.sol\";\nimport \"@openzeppelin/contracts-upgradeable/token/ERC20/IERC20Upgradeable.sol\";\nimport \"./skyverse-signer.sol\";\n\ncontract SkyverseMarketplace is ERC1155Upgradeable, OwnableUpgradeable, skyverseSigner{\n\n    IERC20Upgradeable public skyToken;\n\n    mapping(uint => bool) public isTokenPresent;\n    mapping(uint => uint) public tokenCap;\n    mapping(uint => uint) public CurrentSupplyForEachToken;\n    mapping(address => mapping(uint => uint)) public hasPurchased;\n    mapping(uint => uint) public IndividualCap;\n\n    function initialize(string memory uri_, address _sky) external initializer {\n        __ERC1155_init(uri_);\n        __Ownable_init();\n        __skyverseSigner_init();\n        skyToken = IERC20Upgradeable(_sky);\n    }\n\n    function listNewToken(uint tokenId, uint _max, uint _individualCap) external onlyOwner{\n        require(!isTokenPresent[tokenId], \"Already Listed\");\n        isTokenPresent[tokenId] = true;\n        tokenCap[tokenId] = _max;\n        IndividualCap[tokenId] = _individualCap;\n    }\n\n    function buyNFTs(Transaction memory transaction, uint amount) external onlyOwner{\n        require(isTokenPresent[transaction.tokenId], \"Token does not exist\");\n        require(CurrentSupplyForEachToken[transaction.tokenId] < tokenCap[transaction.tokenId], \"Max Supply Reached\");\n        require(getSigner(transaction) == transaction.userAddress,'Invalid Signature');\n        require(hasPurchased[transaction.userAddress][transaction.tokenId] < IndividualCap[transaction.tokenId],\n            \"Individual Cap reached\");\n        skyToken.transferFrom(transaction.userAddress, address(this), (amount/100) * 1 ether);\n        hasPurchased[transaction.userAddress][transaction.tokenId] += 1;\n        CurrentSupplyForEachToken[transaction.tokenId] += 1;\n        mintTokens(transaction.userAddress, transaction.tokenId, 1);\n    }\n\n    function mintTokens(address _user, uint tokenId, uint _amount) internal{\n        _mint (_user, tokenId, _amount, \"\");\n    }\n\n    ////////////////\n    ////Setters////\n    //////////////\n\n    function changeBaseTokenURI (string memory tokenURI) external onlyOwner{\n        _setURI(tokenURI);\n    }\n\n    function setSkyToken(address _sky) external onlyOwner{\n        skyToken = IERC20Upgradeable(_sky);\n    }\n\n    function setTokenCapOfProject(uint _tokenId,uint _max) external onlyOwner{\n        tokenCap[_tokenId] = _max;\n    }\n\n    function setIndividualCap(uint _tokenId, uint _cap) external onlyOwner{\n        IndividualCap[_tokenId] = _cap;\n    }\n\n    function burnTokens(address _user, uint _tokenId) external onlyOwner {\n        CurrentSupplyForEachToken[_tokenId] -= 1;\n        hasPurchased[_msgSender()][_tokenId] -= 1;\n        _burn(_user, _tokenId, 1);\n    }\n}"
    },
